<p id="notice"><%= notice %></p>

<h1>Listing Inventario Productos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inventario_productos.each do |inventario_producto| %>
      <tr>
        <td><%= inventario_producto.nombre %></td>
        <td><%= inventario_producto.descripcion %></td>
        <td><%= link_to 'Show', inventario_producto %></td>
        <td><%= link_to 'Edit', edit_inventario_producto_path(inventario_producto) %></td>
        <td><%= link_to 'Destroy', inventario_producto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Producto', new_inventario_producto_path %>
